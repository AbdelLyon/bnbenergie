{
  "name": "blank",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/migrate-json-to-payload.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@heroui/accordion": "^2.2.24",
    "@heroui/button": "^2.2.27",
    "@heroui/card": "^2.2.25",
    "@heroui/system": "2.4.23",
    "@heroui/theme": "2.4.23",
    "@payloadcms/db-postgres": "^3.64.0",
    "@payloadcms/next": "^3.64.0",
    "@payloadcms/richtext-lexical": "^3.64.0",
    "@react-three/fiber": "^9.4.0",
    "critters": "^0.0.25",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "framer-motion": "12.23.24",
    "graphql": "^16.8.1",
    "lucide-react": "^0.552.0",
    "next": "15.2.3",
    "nodemailer": "^7.0.10",
    "payload": "^3.64.0",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "^0.34.5",
    "three": "^0.181.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
