{
  "name": "bnbenergie",
  "version": "1.0.0",
  "description": "BNB Energie",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build --webpack",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "db:start": "docker compose up -d postgres",
    "db:stop": "docker compose stop postgres",
    "db:restart": "docker compose restart postgres",
    "db:logs": "docker compose logs -f postgres"
  },
  "dependencies": {
    "@heroui/accordion": "^2.2.24",
    "@heroui/button": "^2.2.27",
    "@heroui/card": "^2.2.25",
    "@heroui/system": "2.4.23",
    "@heroui/theme": "2.4.23",
    "@payloadcms/db-postgres": "^3.64.0",
    "@payloadcms/next": "^3.64.0",
    "@payloadcms/richtext-lexical": "^3.64.0",
    "@payloadcms/storage-vercel-blob": "^3.65.0",
    "@payloadcms/translations": "^3.64.0",
    "@react-three/fiber": "^9.4.0",
    "@tanstack/react-query": "^5.90.11",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "cross-env": "^7.0.3",
    "framer-motion": "12.23.24",
    "graphql": "^16.8.1",
    "lucide-react": "^0.552.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.10",
    "payload": "^3.64.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "^0.34.5",
    "three": "^0.181.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^16.0.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/three": "^0.181.0",
    "critters": "^0.0.25",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.4",
    "knip": "^5.70.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.48.0"
  }
}
